#include <stdio.h>
int main()
{
    int c=0;
    char ch, s[50];
    printf("Enter any string\n");
    gets(s);
    while(s[c]!='\0')
    {
        ch=s[c];
        if(ch>='A' && ch<='Z')
            s[c]=s[c]+32;
        else if (ch>='a' && ch<='z')
            s[c]=s[c]-32;
         c++;
    }
    
    puts(s);
    return 0;
}
